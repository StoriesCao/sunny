<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="CircleProgressView">
        <attr name="radius" format="dimension"/> <!--半径-->
        <attr name="progressBarWidth" format="dimension"/> <!--进度条宽度-->
        <attr name="progressBarColor" format="reference|color"/> <!--进度条颜色-->
        <attr name="innerCircleColor" format="reference|color"/><!--内圆颜色-->
        <attr name="outerCircleColor" format="reference|color"/><!--外圆颜色-->
        <attr name="textColor" format="reference|color"/><!--文本颜色-->
        <attr name="textSize" format="dimension"/><!--文本大小-->
        <attr name="maxProgress" format="integer"/><!--进度条最大值-->
    </declare-styleable>
</resources>
